{
  "name": "@novu/application-generic",
  "version": "0.22.0",
  "description": "Generic backend code used inside of Novu's different services",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "private": false,
  "repository": {
    "url": "https://github.com/novuhq/novu",
    "directory": "packages/nest"
  },
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:copy-template": "cpx \"src/**/*.handlebars\" build/main",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "lint": "eslint src --ext .ts",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "jest src --watch",
    "reset-hard": "git clean -dfx && git reset --hard && pnpm install",
    "prepare-release": "run-s reset-hard test"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "@nestjs/common": "10.2.2",
    "@nestjs/core": ">=10",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/swagger": ">=6",
    "@nestjs/terminus": ">=10",
    "@nestjs/testing": ">=10",
    "@nestjs/passport": "^10.0.1",
    "newrelic": "^9",
    "reflect-metadata": "^0.1.13",
    "jsonwebtoken": "9.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.382.0",
    "@aws-sdk/client-s3": "^3.382.0",
    "@aws-sdk/s3-request-presigner": "^3.382.0",
    "@azure/storage-blob": "^12.11.0",
    "@google-cloud/storage": "^6.2.3",
    "@novu/openim": "*",
    "@novu/wechat": "*",
    "@novu/aliyun-sms": "*",
    "@novu/aliyun-vms": "*",
    "@novu/hongyan-sms": "*",
    "@nestjs/passport": "^10.0.1",
    "@novu/azure-sms": "^0.22.0",
    "@novu/bulk-sms": "^0.22.0",
    "@novu/dal": "^0.22.0",
    "@novu/email-webhook": "^0.22.0",
    "@novu/push-webhook": "^0.22.0",
    "@novu/outlook365": "^0.22.0",
    "@novu/expo": "^0.22.0",
    "@novu/fcm": "^0.22.0",
    "@novu/generic-sms": "^0.22.0",
    "@novu/grafana-on-call": "^0.22.0",
    "@novu/sendgrid": "^0.22.0",
    "@novu/twilio": "^0.22.0",
    "@novu/shared": "^0.22.0",
    "@novu/stateless": "^0.22.0",
    "@novu/testing": "^0.22.0",
    "@sentry/node": "^7.12.1",
    "@segment/analytics-node": "^1.1.4",
    "axios": "^1.6.2",
    "bullmq": "^3.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.2",
    "handlebars": "^4.7.7",
    "ioredis": "^5.2.4",
    "launchdarkly-node-server-sdk": "^7.0.1",
    "lodash": "^4.17.15",
    "mixpanel": "^0.17.0",
    "nestjs-pino": "^3.4.0",
    "node-fetch": "^3.2.10",
    "pino-http": "^8.3.3",
    "pino-pretty": "^9.4.0",
    "recursive-diff": "^1.0.8",
    "redlock": "4.2.0",
    "reflect-metadata": "^0.1.13",
    "rrule": "^2.7.2",
    "rxjs": "7.8.1",
    "@nestjs/passport": "^10.0.1",
    "jsonwebtoken": "9.0.0",
    "i18next": "^23.7.6"
  },
  "optionalDependencies": {
    "@taskforcesh/bullmq-pro": "5.1.14"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "29.5.2",
    "@types/newrelic": "^9",
    "@types/sinon": "^9.0.0",
    "codecov": "^3.5.0",
    "cpx": "^1.5.0",
    "dotenv": "^8.2.0",
    "chai": "^4.2.0",
    "jest": "^27.1.0",
    "newrelic": "^9",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "~2.8.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "ts-jest": "^27.0.5",
    "ts-node": "~10.9.1",
    "typescript": "4.9.5"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "build/main/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
